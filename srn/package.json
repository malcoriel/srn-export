{
  "name": "srn",
  "version": "0.9.8",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "env": "cross-env BROWSERSLIST_IGNORE_OLD_DATA=1",
    "wasm-pack": "yarn env node scripts/wasm-build.js",
    "start-client": "cd client && yarn start",
    "storybook": "cd client && yarn storybook",
    "touch-client": "cd client && touch package.json",
    "start-server": "cd server && (killall -9 srn-server || true) && RUST_BACKTRACE=1 cargo run",
    "watch-server": "cd server && (killall -9 srn-server || true) && cargo-watch -x run",
    "watch-wasm-tests": "cargo-watch -C world -w ../server -s 'yarn wasm-pack forTests'",
    "deploy": "yarn env node scripts/deploy.js",
    "up-version": "yarn env node scripts/up-version.js",
    "regen-resources": "yarn env node scripts/regen-resources.js"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-decorators": "^7.12.13",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@iarna/toml": "^2.2.5",
    "@types/fs-extra": "^9.0.6",
    "@types/yargs": "16",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "common-tags": "^1.8.2",
    "cross-env": "^7.0.3",
    "fs-extra": "^9.0.1",
    "jscodeshift": "^0.11.0",
    "simple-git": "^2.31.0",
    "standard-version": "^9.1.0",
    "typescript": "4.2.3",
    "yargs": "^16.2.0"
  }
}
